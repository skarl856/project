@charset "UTF-8";

/* nanum-gothic-regular - latin */
@font-face {
  font-family: 'Nanum Gothic';
  font-style: normal;
  font-weight: 400;
  src: url('../fonts/nanum-gothic-v17-latin-regular.eot'); /* IE9 Compat Modes */
  src: local('NanumGothic'),
       url('/fonts/nanum-gothic-v17-latin-regular.eot') format('embedded-opentype'), /* IE6-IE8 */
       url('/fonts/nanum-gothic-v17-latin-regular.woff2') /* Super Modern Browsers */
       url('/fonts/nanum-gothic-v17-latin-regular.woff') format('woff'), /* Modern Browsers */
       url('/fonts/nanum-gothic-v17-latin-regular.ttf') format('truetype'), /* Safari, Android, iOS */
       url('/fonts/nanum-gothic-v17-latin-regular.svg#NanumGothic') format('svg'); /* Legacy iOS */
}

/* 구글 머티리얼 아이콘 : 오프라인 */
@font-face {
   font-family: 'Material Icons';
   font-style: normal;
   font-weight: 400;
   src: url(../../css/google/iconfont/MaterialIcons-Regular.eot); /* For IE6-8 */
   src: local('Material Icons'),
        local('MaterialIcons-Regular'),
        url(../../css/google/iconfont/MaterialIcons-Regular.woff2) format('woff2'),
        url(../../css/google/iconfont/MaterialIcons-Regular.woff) format('woff'),
        url(../../css/google/iconfont/MaterialIcons-Regular.ttf) format('truetype');
}
 
.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;  /* Preferred icon size */
  display: inline-block;
  line-height: 1;
  text-transform: none;
  letter-spacing: normal;
  word-wrap: normal;
  white-space: nowrap;
  direction: ltr;
 
  /* Support for all WebKit browsers. */
  -webkit-font-smoothing: antialiased;
  /* Support for Safari and Chrome. */
  text-rendering: optimizeLegibility;
 
  /* Support for Firefox. */
  -moz-osx-font-smoothing: grayscale;
 
  /* Support for IE. */
  font-feature-settings: 'liga';
}

#wrap {
	display: flex;
	z-index: 1;
	width: 1330px;
	margin: 20px auto 0 auto;
    font-family: Nanum Gothic;
}

/* a 태그 데코레이션 제거 */
a, a:hover {
    color: black;
    text-decoration: none;
}

/* 좌측메뉴 */
#left {
    float: left;
    width: 190px;
    height: 750px;
    margin: 10px 30px 10px 0;
    padding: 20px;
    border: 1px solid rgb(141,141,141);
    border-radius: 20px;
}

/* 죄측메뉴 서브 */
#sub_menu {
    font-size: 10pt;
}

/* 우측 본문 */
#container {
    float: left;
    width: 1120px;
    padding: 20px 0;
}

/* 현재창 title */
header {
    margin: 10px 0;
}

/* 컨텐츠 title */
.sub_title {
    margin: 10px 0;
    font-size: 11pt;
}

/* 입력창 */
input[type=text], input[type=password] {
    height: 30px;
    padding: 0 5px;
}

/* 가입정보 테이블 */
#join_info_tbl {
    width: 95%;
    margin: 5px 2% 40px 3%;
    border-top: 1px solid rgb(141,141,141);
}

/* 테이블 카테고리 */
.tbl1 {
    width: 13%;
    max-width: 150px;
    min-width: 130px;
    padding: 15px 20px;
    border-right: 1px solid rgb(141,141,141);
    border-bottom: 1px solid rgb(141,141,141);
    background-color: rgb(203,203,203);
    text-align: right;
}

/* 테이블 내용 */
.tbl2 {
    height: 50px;
    padding: 10px 25px;
    border-bottom: 1px solid rgb(141,141,141);
    font-size: 11pt;
}

/* 라디오 버튼 */
.radio {
    margin: -2px 0 0 0;
    vertical-align: middle;
}

/* 이용/운영 정보 테이블 */
#use_info_tbl {
    width: 95%;
    margin: 5px 2% 40px 3%;
    border-top: 1px solid rgb(141,141,141);
}

/* 셀렉트 박스 */
#select_box {
    width: 60px;
    height: 30px;
    font-size: 10pt;
}

/* 하단부 버튼들 */
#bottom_btns{
    width: 95%;
    height: 31px;
    margin: 5px 2% 50px 3%;
}
.bottom_btn1 {
    float: left;
    width: 100px;
}
.bottom_btn2 {
    float: right;
    width: 100px;
}

/* 팝업창 주변 어둡게*/
#modal, #modal2, #modal3 {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .5);
    text-align: center;
}

/* 로그인 기록 팝업창 */
#modal_ {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 350px;
    height: 550px;
    padding: 1rem 1.5rem;
    border-radius: 0.5rem;
    background-color: white;
}

/* 로그인 기록 팝업창 : 제목 */
#modal_header, .modal_header2 {
    float: left;
    width: 100%;
    padding: 10px 5px 15px;
    border-bottom: 1px solid rgb(141,141,141);
    text-align: left;
}

/* 로그인 기록 팝업창 : 내용 */
#modal_content {
    overflow: hidden;
    height: 400px;
    margin-top: 70px;
    padding: 10px;
    border-radius: 0px;
    text-align: left;
}

/* 로그인 기록 팝업창 : 로그인 기록 박스 */
#modal_content_id {
    overflow-y: scroll;
    width: 100%;
    height: 330px;
    margin: 10px 0 20px;
    padding: 5px;
    border: 1px solid rgb(141,141,141);
}
#modal_content_id table tr {
    height: 30px;
    border-bottom: 1px solid rgb(141,141,141);
}

/* 구매내역 팝업창 */
.modal_2 {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 600px;
    height: 500px;
    padding: 1rem 1.5rem;
    border-radius: 0.5rem;
    background-color: white;
}

/* 구매내역 팝업창 : 내용 */
.modal_content2 {
    overflow: hidden;
    height: 350px;
    margin-top: 70px;
    padding: 10px;
    border-radius: 0px;
    text-align: left;
}

/* 구매내역 팝업창 : 구매내역 박스 */
.modal_content_id2 {
    width: 100%;
    height: 280px;
    margin: 10px 0 20px;
    border-right: 1px solid rgb(141,141,141);
    border-bottom: 1px solid rgb(141,141,141);
}

/* 구매내역 팝업창 : 테이블 */
.buy_hstry_tbl {
    table-layout: fixed;
    width: 100%;
    font-size: 10pt;
    text-align: center;
}
.buy_hstry_tbl2 {
    table-layout: fixed;
    width: 100%;
    font-size: 10pt;
    text-align: center;
}

/* 구매내역 팝업창 : 테이블안 선 처리 */
.buy_hstry_tbl td {
    height: 40px;
    border-top: 1px solid rgb(141,141,141);
    border-left: 1px solid rgb(141,141,141);
    background-color: rgb(203,203,203);
}
.buy_hstry_tbl2 td {
    height: 40px;
    border-top: 1px solid rgb(141,141,141);
    border-left: 1px solid rgb(141,141,141);
}

/* 테이블안 스크롤 */
.buy_hsrty_s {
    overflow-y: scroll;
    width: 100%;
    max-height: 240px;
}
