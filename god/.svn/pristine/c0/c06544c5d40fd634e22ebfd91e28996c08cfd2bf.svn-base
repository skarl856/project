@charset "UTF-8";

/* nanum-gothic-regular - latin */
@font-face {
  font-family: 'Nanum Gothic';
  font-style: normal;
  font-weight: 400;
  src: url('../fonts/nanum-gothic-v17-latin-regular.eot'); /* IE9 Compat Modes */
  src: local('NanumGothic'),
       url('/fonts/nanum-gothic-v17-latin-regular.eot') format('embedded-opentype'), /* IE6-IE8 */
       url('/fonts/nanum-gothic-v17-latin-regular.woff2') /* Super Modern Browsers */
       url('/fonts/nanum-gothic-v17-latin-regular.woff') format('woff'), /* Modern Browsers */
       url('/fonts/nanum-gothic-v17-latin-regular.ttf') format('truetype'), /* Safari, Android, iOS */
       url('/fonts/nanum-gothic-v17-latin-regular.svg#NanumGothic') format('svg'); /* Legacy iOS */
}

/* 구글 머티리얼 아이콘 : 오프라인 */
@font-face {
   font-family: 'Material Icons';
   font-style: normal;
   font-weight: 400;
   src: url(../../css/google/iconfont/MaterialIcons-Regular.eot); /* For IE6-8 */
   src: local('Material Icons'),
        local('MaterialIcons-Regular'),
        url(../../css/google/iconfont/MaterialIcons-Regular.woff2) format('woff2'),
        url(../../css/google/iconfont/MaterialIcons-Regular.woff) format('woff'),
        url(../../css/google/iconfont/MaterialIcons-Regular.ttf) format('truetype');
}
 
.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;  /* Preferred icon size */
  display: inline-block;
  line-height: 1;
  text-transform: none;
  letter-spacing: normal;
  word-wrap: normal;
  white-space: nowrap;
  direction: ltr;
 
  /* Support for all WebKit browsers. */
  -webkit-font-smoothing: antialiased;
  /* Support for Safari and Chrome. */
  text-rendering: optimizeLegibility;
 
  /* Support for Firefox. */
  -moz-osx-font-smoothing: grayscale;
 
  /* Support for IE. */
  font-feature-settings: 'liga';
}

*, ::after, ::before {
	box-sizing: content-box;
}

body {
    font-size: 12px;
    margin: 0;
    padding: 0;
}

a {
    text-decoration: none;
    color: black;
}

a:hover {
	text-decoration: none;
}

li {
    list-style: none;
}

h3 {
    margin: 0;
}

#wrap {
    padding: 70px 20px 50px;
    /* background: cyan; */
}

#title_wrap {
    text-align: center;
    font-size: 20px;
}
/*//////////// 검색 범위 wrap 시작 ////////////////*/

/* 내역 페이지 주문 및 취소/반품/교환 */
#order_result {
    margin-bottom: 30px;
}

/* 내역 페이지 주문 및 취소/반품/교환 영역*/
#order_result ul {
    height: 49px;
    border-bottom: 1px solid;
    padding: 0;
}

/* 내역 페이지 주문 및 취소/반품/교환 좌측 정렬 */
#order_result ul li {
    float: left;
}

/* 내역 페이지 주문 및 취소/반품/교환 버튼 스타일 */
#order_result a {
    display: block;
    width: 210px;
    height: 48px;
    padding: 0 20px;
    text-align: center;
    line-height: 48px;
    border: 1px solid gray;
    border-bottom: 0;
}

/* 누름에 따라 변경될 클래스*/
.order_type_pick {
    background: white;
    border-top: 2px solid black !important;
}

/* 누름에 따라 변경될 클래스*/
.order_type_no {
    background: #e7e7e7;
}

/*//////////// 검색 범위 wrap 끝 ////////////////*/


/*//////////// 날짜에 따른 검색 결과 시작 ////////////////*/
/* 날짜 범위 검색 전체 영역 */
#order_search_days {
    border: 1px solid #e7e7e7;
    padding: 20px;
}

/* 주문상태 범위 검색 영역 */
#order_search_select {
    display: inline-block;
    padding-right: 12px;
}

/* 주문상태 범위 검색 select */
#order_search_select select {
    height: 24px;
}

/* 날짜 기간 영역 */
#search_day_btn {
    margin-right: 20px;
}

/* 날짜 기간 영역 버튼 */
#search_day_btn a {
    border: 1px solid #ddd;
    padding: 4px;
    margin: -2px;
}

/* 날짜 범위 입력란 */
[id^=history_] {
    width: 120px;
}

/* 날짜 검색 버튼 */
.ui-datepicker-trigger {
    margin-right: 15px;
    vertical-align: middle;
}

/* 검색 버튼 */
#orderSearchBtn {
    border: 0;
    background: black;
    color: white;
}

#search_info_content {
	margin: 20px;
}

/* 설명란 */
#search_info_content li {
    list-style: disc;
    color: #ddd;
 
}

/*//////////// 날짜에 따른 검색 결과 끝 ////////////////*/

/*//////////// 사용자 주문처리 리스트 시작 ////////////////*/
/* 사용자 주문처리 리스트 영역 */
#order_result_list {
    width: 100%;
    border-top: 2px solid black;
    text-align: center;
}

/* 사용자 주문처리 리스트 간격 */
#order_result_list th {
    padding: 16px 0;
    border-bottom: 1px solid #e7e7e7;
}

/* 사용자 주문처리 리스트 이미지 사이즈 */
#order_result_list img {
    width: 100px;
    height: 100px;
}
/*//////////// 사용자 주문처리 리스트 끝 ////////////////*/
#pagination {
	text-align: center;
	margin: 30px 0;
}

#pagination a {
	padding-left: 10px;
}
